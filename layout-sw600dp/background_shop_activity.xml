<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_shop_background"
    tools:context=".TreeShopActivity">


    <TextView
        android:id="@+id/money_bar_background_shop"
        android:layout_width="350dp"
        android:layout_height="80dp"

        android:autoSizeTextType="uniform"

        android:fontFamily="@font/bungee_inline"
        android:gravity="right"
        android:textColor="#4CAF50"
        android:textSize="28sp"
        app:drawableRightCompat="@drawable/money_shop_icon_big"
        app:layout_constraintBottom_toTopOf="@+id/guideline4"
        app:layout_constraintEnd_toStartOf="@+id/back_background_shop_button"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.313" />

    <ImageView
        android:id="@+id/default_tree_image"
        android:layout_width="280dp"
        android:layout_height="280dp"
        app:layout_constraintEnd_toEndOf="@+id/cyan_tree_image"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/cyan_tree_image"
        app:layout_constraintTop_toTopOf="@+id/guideline4"
        app:srcCompat="@drawable/default_background_frame" />

    <ImageView
        android:id="@+id/blue_tree_image"
        android:layout_width="280dp"
        android:layout_height="280dp"
        app:layout_constraintEnd_toEndOf="@+id/purple_tree_image"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/purple_tree_image"
        app:layout_constraintTop_toTopOf="@+id/default_tree_image"
        app:srcCompat="@drawable/desert_background_frame" />

    <ImageView
        android:id="@+id/cyan_tree_image"
        android:layout_width="280dp"
        android:layout_height="280dp"
        app:layout_constraintEnd_toEndOf="@+id/green_tree_image"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/green_tree_image"
        app:layout_constraintTop_toBottomOf="@+id/default_tree_image"
        app:srcCompat="@drawable/desert2_background_frame" />


    <ImageView
        android:id="@+id/purple_tree_image"
        android:layout_width="280dp"
        android:layout_height="280dp"
        app:layout_constraintEnd_toEndOf="@+id/orange_tree_image"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/orange_tree_image"
        app:layout_constraintTop_toBottomOf="@+id/blue_tree_image"
        app:srcCompat="@drawable/farmnature_background_frame" />

    <ImageView
        android:id="@+id/green_tree_image"
        android:layout_width="280dp"
        android:layout_height="280dp"
        app:layout_constraintEnd_toEndOf="@+id/red_tree_image"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/red_tree_image"
        app:layout_constraintTop_toBottomOf="@+id/cyan_tree_image"
        app:srcCompat="@drawable/rainbow_background_frame" />

    <ImageView
        android:id="@+id/orange_tree_image"
        android:layout_width="280dp"
        android:layout_height="280dp"
        app:layout_constraintEnd_toEndOf="@+id/yellow_tree_image"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/yellow_tree_image"
        app:layout_constraintTop_toBottomOf="@+id/purple_tree_image"
        app:srcCompat="@drawable/coming_soon_frame" />

    <ImageView
        android:id="@+id/red_tree_image"
        android:layout_width="280dp"
        android:layout_height="280dp"
        app:layout_constraintBottom_toBottomOf="@+id/yellow_tree_image"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/guideline8"
        app:layout_constraintTop_toBottomOf="@+id/green_tree_image"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/coming_soon_frame" />

    <ImageView
        android:id="@+id/yellow_tree_image"
        android:layout_width="280dp"
        android:layout_height="280dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/red_tree_image"
        app:layout_constraintHorizontal_bias="0.395"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/orange_tree_image"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/coming_soon_frame" />

    <TextView
        android:id="@+id/frame_text5"
        android:layout_width="170dp"
        android:layout_height="35dp"
        android:drawablePadding="-20dp"
        android:fontFamily="@font/architects_daughter"
        android:gravity="center"
        android:text="31,313,000"
        android:textColor="#FFFFFF"
        android:textSize="28sp"
        app:drawableRightCompat="@drawable/money_shop_icon"
        app:layout_constraintBottom_toBottomOf="@+id/green_tree_image"
        app:layout_constraintEnd_toEndOf="@+id/green_tree_image"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="@+id/green_tree_image"
        app:layout_constraintTop_toBottomOf="@+id/cyan_tree_image"
        app:layout_constraintVertical_bias="0.843" />

    <TextView
        android:id="@+id/frame_text4"
        android:layout_width="170dp"
        android:layout_height="35dp"
        android:drawablePadding="-20dp"
        android:fontFamily="@font/architects_daughter"
        android:gravity="center"
        android:text="1,012,000"
        android:textColor="#FFFFFF"
        android:textSize="28sp"
        app:drawableRightCompat="@drawable/money_shop_icon"
        app:layout_constraintBottom_toBottomOf="@+id/purple_tree_image"
        app:layout_constraintEnd_toEndOf="@+id/purple_tree_image"
        app:layout_constraintHorizontal_bias="0.542"
        app:layout_constraintStart_toStartOf="@+id/purple_tree_image"
        app:layout_constraintTop_toBottomOf="@+id/blue_tree_image"
        app:layout_constraintVertical_bias="0.829" />

    <TextView
        android:id="@+id/frame_text3"
        android:layout_width="170dp"
        android:layout_height="35dp"
        android:drawablePadding="-20dp"
        android:fontFamily="@font/architects_daughter"
        android:gravity="center"
        android:text="308,000"
        android:textColor="#FFFFFF"
        android:textSize="28sp"
        app:drawableRightCompat="@drawable/money_shop_icon"
        app:layout_constraintBottom_toBottomOf="@+id/cyan_tree_image"
        app:layout_constraintEnd_toEndOf="@+id/cyan_tree_image"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="@+id/cyan_tree_image"
        app:layout_constraintTop_toBottomOf="@+id/default_tree_image"
        app:layout_constraintVertical_bias="0.828" />

    <TextView
        android:id="@+id/frame_text2"
        android:layout_width="170dp"
        android:layout_height="35dp"
        android:drawablePadding="-20dp"
        android:fontFamily="@font/architects_daughter"
        android:gravity="center"
        android:text="100,000"
        android:textColor="#FFFFFF"
        android:textSize="28sp"
        app:drawableRightCompat="@drawable/money_shop_icon"
        app:layout_constraintBottom_toBottomOf="@+id/blue_tree_image"
        app:layout_constraintEnd_toEndOf="@+id/blue_tree_image"
        app:layout_constraintHorizontal_bias="0.542"
        app:layout_constraintStart_toStartOf="@+id/blue_tree_image"
        app:layout_constraintTop_toTopOf="@+id/blue_tree_image"
        app:layout_constraintVertical_bias="0.836" />

    <TextView
        android:id="@+id/frame_text1"
        android:layout_width="170dp"
        android:layout_height="35dp"
        android:fontFamily="@font/architects_daughter"
        android:gravity="center"
        android:paddingRight="-10dp"
        android:text="Selected"
        android:textColor="#FFFFFF"
        android:textSize="28dp"
        app:layout_constraintBottom_toBottomOf="@+id/default_tree_image"
        app:layout_constraintEnd_toEndOf="@+id/default_tree_image"
        app:layout_constraintHorizontal_bias="0.536"
        app:layout_constraintStart_toStartOf="@+id/default_tree_image"
        app:layout_constraintTop_toTopOf="@+id/default_tree_image"
        app:layout_constraintVertical_bias="0.853" />

    <ImageView
        android:id="@+id/back_background_shop_button"
        android:layout_width="115dp"
        android:layout_height="80dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline4"
        app:layout_constraintEnd_toEndOf="@+id/default_tree_image"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.313"
        app:srcCompat="@drawable/back" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="131dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="384dp" />


</androidx.constraintlayout.widget.ConstraintLayout>